{
    "source": "./src",
    "index": "./README-esdoc.md",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "manual": {
            "files": [
              "./documents/options.md",
              "./documents/controllers.md",
              "./documents/middlewares.md",
              "./documents/services.md"
            ]
          }
        }
      },
      {"name": "esdoc-node"}
    ]
}
